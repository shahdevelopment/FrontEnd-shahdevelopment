git rm -r --cached *

git remote remove origin


git branch -a

git branch -d <branch_name>

git tag -l


git tag -d <tag_name>

# will return the .git dir if there is one
dir /A:D /B

rmdir /S /Q .git
git init

git commit -m "first commit"

git branch -M main

git remote add origin git@github.com:Shah0373/profile_front.git

git push -u origin main


git checkout --detach main